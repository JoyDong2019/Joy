高速公路超速处罚
#include<stdio.h>
int main(void)
{
	int v,n;
	double x;
	int y;
	printf("请输入车速和限速："); scanf_s("%d %d", &v, &n);
	x = 1.0 * v / n;
	y = (x - 1) * 100;
	if (x < 1.1)
		printf("OK");
	else if (x >= 1.1 && x < 1.5)
		printf("Exceed%d%%.Ticket 200", y);/*如何输出百分号*/
	else
		printf("Exceed%d%%.License Revoked", y);
	return(0);
}
